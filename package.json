{
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone-forms": "^0.14.1",
    "jquery-ui": "^1.12.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "node-sass": "^4.5.0",
    "pug-cli": "^1.0.0-alpha6"
  },
  "scripts": {
    "build": "for CMD in \"build-css\" \"build-js\"; do npm run ${CMD}; done",
    "build-css": "node-sass --output-style expanded -- src/jquery-ui.sass > jquery-ui.css",
    "build-js": "coffee --bare --compile --no-header --output . -- src/*.coffee",
    "build-docs": "pug --out docs templates/*.pug; coffee --bare --compile --no-header --output docs -- src/*.coffee; node-sass --output-style expanded -- src/jquery-ui.sass > docs/jquery-ui.css"
  },
  "name": "backbone-forms-jquery-ui",
  "description": "",
  "version": "1.0.0",
  "repository": "git+https://github.com/tomi77/backbone-forms-jquery-ui.git",
  "keywords": [
    "backbone-forms",
    "jquery-ui",
    "autocomplete",
    "checkboxradio",
    "datepicker",
    "selectmenu",
    "slider",
    "spinner"
  ],
  "author": "Tomasz Jakub Rup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/backbone-forms-jquery-ui/issues"
  },
  "homepage": "https://github.com/tomi77/backbone-forms-jquery-ui"
}
