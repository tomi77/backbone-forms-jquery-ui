{
  "dependencies": {
    "jquery-ui": "^1.12.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "coffee-script": "^1.12.0",
    "cucumber": "^3.0.0",
    "node-sass": "^4.5.0",
    "pug-cli": "^1.0.0-alpha6",
    "selenium-webdriver": "^3.5.0"
  },
  "peerDependencies": {
    "backbone": "^1.3.3",
    "backbone-forms": "^0.14.1"
  },
  "scripts": {
    "test": "cucumber.js --compiler coffee:coffee-script/register --tags \"not @ignore\"",
    "build": "for CMD in \"build:css\" \"build:js\" \"build:docs\"; do npm run ${CMD}; done",
    "build:css": "node-sass --output-style expanded -- src/jquery-ui.sass > jquery-ui.css",
    "build:js": "coffee --bare --compile --no-header --output . -- src/*.coffee",
    "build:docs": "pug --out docs templates/*.pug; cp *.css docs/; cp *.js docs/"
  },
  "name": "backbone-forms-jquery-ui",
  "description": "A Backbone-Forms jQuery UI related editors",
  "version": "1.0.0",
  "repository": "tomi77/backbone-forms-jquery-ui",
  "keywords": [
    "backbone-forms",
    "jquery-ui",
    "autocomplete",
    "checkboxradio",
    "datepicker",
    "selectmenu",
    "slider",
    "spinner"
  ],
  "author": "Tomasz Jakub Rup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/backbone-forms-jquery-ui/issues"
  },
  "homepage": "https://github.com/tomi77/backbone-forms-jquery-ui"
}
